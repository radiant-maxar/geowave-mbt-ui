  "author": "DigitalGlobe",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/enzyme": "^2.8.6",
    "@types/jest": "^20.0.6",
    "@types/react": "^15.6.1",
    "@types/react-dom": "^15.5.2",
    "@types/react-test-renderer": "^15.5.2",
    "@types/sinon": "^2.3.3",
    "awesome-typescript-loader": "^3.2.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.4.1",
    "style-loader": "^0.18.1",
    "ts-jest": "^20.0.10",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.5.2",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "core-js": "^2.5.0",
    "leaflet": "^1.2.0",
    "leaflet-draw": "^0.4.12",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*(\\.test|Test)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/test/utils/file-mock.ts",
      "\\.(css|less)$": "<rootDir>/test/utils/style-mock.ts"
    }
  }
}
